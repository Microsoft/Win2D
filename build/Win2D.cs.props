<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!-- [[NOT IN SAMPLE]] -->

  <PropertyGroup>
    <OutputPath>$(__OutputPath)</OutputPath>
    <OutDir>$(__OutputPath)</OutDir>
    <GenerateProjectSpecificOutputFolder>false</GenerateProjectSpecificOutputFolder>
  </PropertyGroup>

  <!-- [[IN SAMPLE]] -->

  <PropertyGroup>
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <TargetPlatformVersion Condition="'$(TargetPlatformVersion)' == ''">$(__TargetPlatformVersion)</TargetPlatformVersion>
    <TargetPlatformMinVersion Condition="'$(TargetPlatformMinVersion)' == ''">$(__TargetPlatformMinVersion)</TargetPlatformMinVersion>
  </PropertyGroup>
  
</Project>
